	<queryString>
		<![CDATA[select a.*,s.sbu_name,l.loc_name from
(select a.sbucod,b.loccod,a.advcod,b.shrtnm,b.agncls,b.subdcd,c.subdes,d.poldat,a.polnum,a.prdcod,concat(year(d.poldat)," / ",a.prdcod," / ",a.polnum) pol,
       a.fypach,a.precoly1,a.preduely1,0 as retper,0 as incper,0 as incamt,date_format(a.txndat,'%M-%Y') txndat
from inprodincentive_details a
inner join inagentmast b on a.sbucod=b.sbucod and a.advcod=b.agncod
inner join insubdesignation c on a.sbucod=c.sbucod and b.subdcd=c.subdcd
inner join inproposals d on a.sbucod=d.sbucod and a.polnum=d.polnum and d.pprsta<>'INAC'
where a.sbucod=$P{sbucod} and b.agncls in ('IC','UNL') and a.txndat=$P{date} and b.loccod like $P{loccod} and a.advcod like $P{agncod}

union all

select a.sbucod,b.loccod,a.advcod,b.shrtnm,b.agncls,b.subdcd,c.subdes,0 as poldat,0 as polnum,0 as prdcod,'A' as pol,
       0 as fypach,0 as precoly1,0 as preduely1,retper,incper,incamt,date_format(a.txndat,'%M-%Y') txndat
from inprodincentive_sum a
inner join inagentmast b on a.sbucod=b.sbucod and a.advcod=b.agncod
inner join insubdesignation c on a.sbucod=c.sbucod and b.subdcd=c.subdcd
where a.sbucod=$P{sbucod} and b.agncls in ('IC','UNL') and a.txndat=$P{date} and b.loccod like $P{loccod} and a.advcod like $P{agncod})a

inner join rms_sbu s on a.sbucod=s.sbu_code
inner join rms_locations l on a.sbucod=l.sbu_code and a.loccod=l.loc_code]]>
	</queryString>
